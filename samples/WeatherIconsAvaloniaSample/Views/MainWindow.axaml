					<wi:WeatherIcon Tertiary="OrangeRed" Quaternary="LightSeaGreen" Key="NightThunderstorm"/>
					<wi:WeatherIcon Key="NightCloudyHigh"/>
					<wi:WeatherIcon Key="NightPartlyCloudy"/>
				</WrapPanel>
