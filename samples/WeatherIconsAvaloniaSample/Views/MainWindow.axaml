		<Expander Header="Miscellaneous" IsExpanded="False">
			<WrapPanel>
				<wi:WeatherIcon Key="Alien"/>
				<wi:WeatherIcon Key="Celsius"/>
				<wi:WeatherIcon Key="Fahrenheit"/>
